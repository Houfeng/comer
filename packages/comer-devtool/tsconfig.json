{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./lib/",
    "declarationDir": "./types/",
    "target": "ES5",
    "lib": [
      "ESNext",
      "DOM",
      "WebWorker",
    ],
    "types": [
      "./node_modules/chrome-types/"
    ]
  },
  "include": [
    "./src/**/*.ts",
  ],
  "exclude": [
    "**/node_modules/**/*.*",
    "**/types/**/*.*"
  ]
}